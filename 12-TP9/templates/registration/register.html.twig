
{% extends "base.html.twig" %}

{% block title %}Inscription{% endblock %}

{% block body %}
    <div>
        {% for message in app.flashes('success') %}
            <div class="flash-notice">
                {{ message }}
            </div>
        {% endfor %}
        {{ form_start(form) }}
        {{ form_row(form) }}
        <button type="submit">Envoyer</button>
        {{ form_end(form) }}

    </div>
{% endblock %}